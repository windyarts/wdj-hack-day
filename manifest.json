{
    "name": "时光放映机",
    "version": "1.0",
    "manifest_version": "2",

    "icons": {
        "36": "img/icon36.png",
        "48": "img/icon48.png",
        "72": "img/icon72.png",
        "96": "img/icon96.png"
    },

    "content_scripts": [
        {
            "matches": [
                "http://www.wandoujia.com/blank.html"
            ],
            "css": [
                "css/style.css"
            ],
            "js": [
                "config.js",
                "client/javascripts/jquery.js",
                "server/photosync.js"
            ]
        },
        {
            "matches": [
                "http://tisafu.com/hackday/flickr.php"
            ],
            "js": [
                "config.js",
                "client/javascripts/jquery.js",
                "client/javascripts/imageloader.js",
                "server/flickr.js"
            ]
        },
        {
            "matches": [
                "http://tisafu.com/hackday/main.php"
            ],
            "js": [
                "config.js",
                "client/javascripts/template.js",
                "client/javascripts/jquery.js",
                "client/javascripts/imageloader.js",
                "client/javascripts/main.js"
            ],
            "run_at": "document_end"
        }
    ],

    "app": {
        "urls": [
            
        ],
        "launch": {
            "web_url": "http://tisafu.com/hackday/cover.html"
        }
    }

}